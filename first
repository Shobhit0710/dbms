DROP TABLE CLIENT_MASTER 
CREATE TABLE CLIENT_MASTER( CLIENTNO VARCHAR2(10),NAME VARCHAR2(20),CITY VARCHAR2(10),PINCODE NUMBER(10),STATE VARCHAR2(10),
BALDUE NUMBER(10)); 
SELECT * FROM CLIENT_MASTER
