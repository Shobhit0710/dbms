CREATE TABLE Product_master(product_no VARCHAR2(6),
DESCRIPTION VARCHAR2(20),
PROFIT_PERCENTAGE VARCHAR2(6),unit_measure VARCHAR2(10),
qty_on_hand VARCHAR2(6),
recorder_lv1 NUMBER(6),
sell_price number(7,2),
cost_price NUMBER(7,2));
select * FROM product_master
ALTER TABLE CLIENT_MASTER MODIFY CLIENT_NO NUMBER(10); 
